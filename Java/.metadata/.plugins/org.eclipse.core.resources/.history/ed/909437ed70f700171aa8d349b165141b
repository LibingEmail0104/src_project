package logintestapi;

public class Fdf {

	public static void main(String[] args) {

		sum(55);

	}

	public static void sum(int m) {
		int sum = 0;
		int i = 0;
		// int j = 0;
		for (i = 0; i <= m; i++) {
			sum = sum + i;
		}

		// for (j = 0; j <= 100; j++) {
		// if (j % 2 != 0) {
		// System.out.println("j is " + j);
		// }
		// }

		System.out.println(sum);
	}

}

